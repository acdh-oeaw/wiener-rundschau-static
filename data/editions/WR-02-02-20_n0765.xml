   <text>
      <body>
         <div type="page">
            <fw type="hd">
               <fw type="ctext">ORPHISCHES LIED</fw>
               <fw type="pn">765</fw>
            </fw>
            <lg>
               <l>Dann</l>
               <l>schleicht die schwarze nacht heran</l>
               <l>und scheucht den leidenswind hin- weg</l>
               <l>und raubt die blauen berge des sehnens</l>
               <l>und trinkt die wogen der liebes- wonne /</l>
               <l>die singenden /</l>
               <l>die sonnegeküssten <milestone unit="3dots"/>
               </l>
            </lg>
            <lg>
               <l>
                  <milestone unit="2dots"/> Leise stöhnt</l>
               <l>die föhre im dunkel</l>
               <l>und klagt zu den Sternen <milestone unit="3dots"/>
               </l>
            </lg>
            <lg>
               <l>— Wie eine föhre stöhnt</l>
               <l>meine seele im dunkel</l>
               <l>und klagt zu den Sternen <milestone unit="3dots"/>
               </l>
            </lg>
            <p>(<date>1891</date>.) <rs type="person" key="">Constantin Christomanos</rs>.</p>
            <milestone unit="untyped-end"/>
            <milestone unit="hr"/>
         </div>
